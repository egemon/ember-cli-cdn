    "config": "config",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "mocha tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dakal-oleksandr/ember-cli-cdn.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Dakal Oleksandr <dakal.oleksandr@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ember-addon",
    "cdn",
    "cdnizer",
    "vendor",
    "scripts",
    "styles"
  ],
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "loader.js": "^4.0.10",
    "ember-cli": "2.9",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.1.0",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-ic-ajax": "0.2.4",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^1.0.4",
    "ember-cli-release": "0.2.8",
    "ember-cli-sri": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "1.13.15",
