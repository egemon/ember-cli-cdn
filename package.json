    "ember-try": "~0.0.8",
    "chai": "^3.4.1",
    "rimraf": "^2.4.3"
  },
  "dependencies": {
    "broccoli-cdnizer": "^1.0.0",
    "broccoli-funnel": "^1.0.0",
    "broccoli-merge-trees": "^1.0.0",
    "broccoli-unwatched-tree": "^0.1.1",
    "ember-cli-babel": "^5.1.5",
    "lodash": "^3.10.1",
